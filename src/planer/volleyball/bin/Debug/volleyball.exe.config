<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="volleyball.app" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
		</sectionGroup>
	</configSections>
	<startup useLegacyV2RuntimeActivationPolicy="true">
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
	</startup>
	<appSettings>
		<add key="Title" value="Turnierplaner V" />
		<add key="Versionnr" value="13" />
		<add key="About" value="(c) cfr" />
		<add key="Version" value="./resources/version.txt" />
		<add key="Logging" value="./resources/logging.txt" />
		<add key="Database" value="Data Source=./resources/data.db;Version=3;" />
		
		<add key="FTPDatabase" value="./resources/upload/data.db" />
		<add key="FTPUrl" value="ftp://vb4222.at/html/vb/data.db" />
		<add key="FTPUser" value="38730ftp1" />
		<add key="FTPPw" value="550spyder" />
		
		<add key="SelectTeams" value="SELECT * FROM mannschaften ORDER BY id" />
		<add key="UpdateTeams" value="UPDATE mannschaften SET a = @A, b = @B, c = @C, d = @D, e = @E, f = @F, g = @G, h = @H, i = @I, j = @J, k = @K, l = @L WHERE id = @ID" />
		<add key="DeleteTeams" value="DELETE FROM mannschaften" />
		<add key="CountTeams" value="SELECT * FROM team_count_view" />
		<add key="CountDivisions" value="SELECT * FROM division_count_view" />
		
		<add key="CountFields" value="SELECT COUNT(id) FROM felder" />
		<add key="SelectFields" value="SELECT * FROM felder ORDER BY id ASC" />
		<add key="InsertField" value="INSERT INTO felder VALUES(@ID,'')" />
		<add key="DeleteField" value="DELETE FROM felder WHERE id &gt;" />		
	
		<add key="SelectResults" value="SELECT * FROM @RUNDE ORDER BY punkte DESC, satz DESC, intern ASC" />
		<add key="UpdateResults" value="UPDATE @RUNDE SET ms = @MS, punkte = @PUNKTE, satz = @SATZ, intern = @INTERN, extern = @EXTERN WHERE id = @ID" />
		
		<add key="SelectAllResults" value="SELECT * FROM vorrunde_all_view" />
		
		<add key="SelectQualifying" value="SELECT * FROM vorrunde_spielplan ORDER BY id" />
		<add key="UpdateQualifying" value="UPDATE vorrunde_spielplan SET runde = @RUNDE, spiel = @SPIEL, zeit = @ZEIT, feldnummer = @FELDNUMMER, feldname = @FELDNAME, ms_a = @MSA, ms_b = @MSB, sr = @SR, satz1a = @SATZ1A, satz1b = @SATZ1B, satz2a = @SATZ2A, satz2b = @SATZ2B, satz3a = @SATZ3A, satz3b = @SATZ3B WHERE id = @ID" />
		<add key="DeleteQualifying" value="DELETE * FROM vorrunde_spielplan" />
		<add key="QualifyingMaxTime" value="SELECT MAX(zeit) FROM vorrunde_spielplan ORDER BY id" />
	
		<add key="SelectInterim" value="SELECT * FROM zwischenrunde_spielplan ORDER BY id" />
		<add key="UpdateInterim" value="UPDATE zwischenrunde_spielplan SET runde = @RUNDE, spiel = @SPIEL, zeit = @ZEIT, feldnummer = @FELDNUMMER, feldname = @FELDNAME, ms_a = @MSA, ms_b = @MSB, sr = @SR, satz1a = @SATZ1A, satz1b = @SATZ1B, satz2a = @SATZ2A, satz2b = @SATZ2B, satz3a = @SATZ3A, satz3b = @SATZ3B WHERE id = @ID" />
		<add key="DeleteInterim" value="DELETE * FROM zwischenrunde_spielplan" />
		<add key="InterimMaxTime" value="SELECT MAX(zeit) FROM zwischenrunde_spielplan ORDER BY id" />
		<add key="InterimLastRoundData" value="SELECT runde, spiel, zeit FROM zwischenrunde_spielplan ORDER BY id DESC LIMIT 1" />
		<add key="InterimGamesCount" value="SELECT * FROM zwischenrunde_spielplan" />
		<add key="InterimGameMaxTime" value="SELECT MAX(zeit) FROM zwischenrunde_spielplan ORDER BY id" />
		
		<add key="SelectPreClassement" value="SELECT * FROM kreuzspiele_spielplan ORDER BY id" />
		<add key="UpdatePreClassement" value="UPDATE kreuzspiele_spielplan SET runde = @RUNDE, spiel = @SPIEL, zeit = @ZEIT, feldnummer = @FELDNUMMER, feldname = @FELDNAME, ms_a = @MSA, ms_b = @MSB, sr = @SR, satz1a = @SATZ1A, satz1b = @SATZ1B, satz2a = @SATZ2A, satz2b = @SATZ2B, satz3a = @SATZ3A, satz3b = @SATZ3B WHERE id = @ID" />
		<add key="DeletePreClassement" value="DELETE * FROM kreuzspiele_spielplan" />
		<add key="PreClassementLastRoundData" value="SELECT runde, spiel, zeit FROM zwischenrunde_spielplan ORDER BY id DESC LIMIT 1" />
		<add key="PreClassementCount" value="SELECT * FROM kreuzspiele_spielplan" />
		<add key="PreClassementMaxTime" value="SELECT MAX(zeit) FROM kreuzspiele_spielplan ORDER BY id" />
		
		<add key="SelectClassement" value="SELECT * FROM platzspiele_spielplan ORDER BY id" />
		<add key="UpdateClassement" value="UPDATE platzspiele_spielplan SET runde = @RUNDE, spiel = @SPIEL, zeit = @ZEIT, feldnummer = @FELDNUMMER, feldname = @FELDNAME, ms_a = @MSA, ms_b = @MSB, sr = @SR, satz1a = @SATZ1A, satz1b = @SATZ1B, satz2a = @SATZ2A, satz2b = @SATZ2B, satz3a = @SATZ3A, satz3b = @SATZ3B WHERE id = @ID" />
		<add key="DeleteClassement" value="DELETE * FROM platzspiele_spielplan" />
		<add key="ClassementLastRoundData" value="SELECT runde, spiel, zeit FROM kreuzspiele_spielplan ORDER BY id DESC LIMIT 1" />
		<add key="ClassementGamesCount" value="SELECT * FROM platzspiele_spielplan" />
		<add key="ClassementMaxTime" value="SELECT MAX(zeit) FROM platzspiele_spielplan ORDER BY id" />
	</appSettings>
